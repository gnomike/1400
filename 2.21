import math

def calculate_functions(e, f, g, h):
    a = (e + f / 2) / 3
    b = abs(h**2 - g)
    c = math.sqrt((g - h)**2 - 3 * math.sin(e))
    return a, b, c

# Пример использования
e = 1.0
f = 2.0
g = 3.0
h = 4.0

a, b, c = calculate_functions(e, f, g, h)
print(f"a = {a}")
print(f"b = {b}")
print(f"c = {c}")

import math

def find_hypotenuse():
    a = float(input("Введите длину первого катета: "))
    b = float(input("Введите длину второго катета: "))
    
    if a <= 0 or b <= 0:
        print("Ошибка: длины катетов должны быть положительными!")
        return
    
    c = math.sqrt(a**2 + b**2)
    print(f"Гипотенуза: {c:.2f}")

find_hypotenuse()

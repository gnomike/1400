import math

# Ввод данных
a = float(input("Введите значение a: "))
b = float(input("Введите значение b: "))

# Проверка допустимости значений
if b < 0:
    print("Ошибка: b должно быть неотрицательным.")
elif (math.sqrt(b) + (a + b) / 2) == 0:
    print("Ошибка: знаменатель в выражении для x равен нулю.")
else:
    # Вычисление x
    numerator_x = 2 / (a**2 + 25) + b
    denominator_x = math.sqrt(b) + (a + b) / 2
    x = numerator_x / denominator_x

    # Вычисление y
    numerator_y = abs(a) + 2 * math.sin(b)
    denominator_y = 5.5 * a
    y = numerator_y / denominator_y

    # Вывод результатов
    print(f"x = {x}")
    print(f"y = {y}")

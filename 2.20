import math

def calculate_functions(e, f, g, h):
    # Проверка допустимости значений
    if f == 0:
        return "Ошибка: f не может быть равно нулю."
    if e * f - 3 == 0:
        return "Ошибка: ef - 3 не может быть равно нулю."

    # Вычисление функции a
    a = math.sqrt(abs(e - 3 / f) ** 3 + g)

    # Вычисление функции b
    b = math.sin(e) + math.cos(h) ** 2

    # Вычисление функции c
    c = 33 * g / (e * f - 3)

    return a, b, c

# Пример использования
e = float(input("Введите значение e: "))
f = float(input("Введите значение f: "))
g = float(input("Введите значение g: "))
h = float(input("Введите значение h: "))

result = calculate_functions(e, f, g, h)
if isinstance(result, str):
    print(result)
else:
    a, b, c = result
    print(f"a = {a}")
    print(f"b = {b}")
    print(f"c = {c}")

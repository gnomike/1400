# Размеры прямоугольника
length = 543  # длина в мм
width = 130   # ширина в мм

# Размер стороны квадрата
square_side = 130

# Вычисление количества квадратов
count_squares = length // square_side  # целочисленное деление

# Вывод результата
print(f"Можно отрезать {count_squares} квадратов со стороной 130 мм")
print(f"Остаток длины: {length % square_side} мм")
